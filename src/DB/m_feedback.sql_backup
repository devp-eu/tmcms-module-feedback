CREATE TABLE `m_feedback` (  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,  `client_id` int(10) unsigned NOT NULL,  `date_created` int(10) unsigned NOT NULL,  `done` tinyint(1) unsigned NOT NULL DEFAULT '0',  `name` varchar(255) NOT NULL,  `email` varchar(255) NOT NULL,  `phone` varchar(30) NOT NULL,  `message` text NOT NULL,  PRIMARY KEY (`id`),  KEY `client_id` (`client_id`)) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8
